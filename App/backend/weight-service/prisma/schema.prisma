generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Weight {
  id            String   @id @default(uuid()) @db.Uuid
  date          DateTime
  weight        Float
  userId        String
  bloodPressure String?
  bmi           Float?
  chest         Float?
  heartRate     Int?
  hip           Float?
  thigh         Float?
  waist         Float?
}
